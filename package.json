{

  "name": "shop-assistant-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://gitlab.nfq.lt/omni/shop-assistant-client"
  },
  "scripts": {
    "flow:setup": "flow-typed install && flow-typed update",
    "flow:update": "flow-typed update",
    "flow": "flow",
    "flow:errors": "flow --show-all-errors",
    "flow:coverage": "flow coverage ./client/index.js --color && flow-coverage-report -i client/**/*.js -x client/**/*.test.js -x client/**/*.spec.js -t html",
    "prepush": "yarn test:coverage",
    "test": "jest",
    "test:changed": "yarn test --onlyChanged --passWithNoTests --silent --runInBand",
    "test:watch": "yarn test --watch",
    "test:update": "yarn test --update",
    "test:coverage": "yarn test --coverage --verbose --silent --runInBand",
    "lint": "eslint \"client/**/*.js\" --cache",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint:fix --max-warnings=0",
    "precommit": "yarn test:changed && lint-staged",
    "postinstall" : "yarn flow:setup",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --content-base build --env=dev",
    "build": "rimraf ./public/* && cross-env NODE_ENV=production webpack --env=prod"
  },
  "lint-staged": {
    "client/**/*.{js,jsx}": [
      "lint:staged",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tools/jest/jest.setup.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "empty": "^0.10.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^1.4.8",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-with-prettier": "^2.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.63.1",
    "flow-coverage-report": "^0.4.1",
    "flow-typed": "^2.2.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
}
