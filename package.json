{
  "name": "shop-assistant-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://gitlab.nfq.lt/omni/shop-assistant-client"
  },
  "scripts": {
    "flow:setup": "flow-typed install",
    "flow:update": "flow-typed update",
    "flow": "flow",
    "flow:errors": "flow --show-all-errors",
    "flow:coverage": "flow coverage ./client/index.js --color && flow-coverage-report -i client/**/*.js -x client/**/*.test.js -x client/**/*.spec.js -t html",
    "prepush": "yarn test:coverage",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:update": "yarn test --update",
    "test:coverage": "yarn test --coverage --verbose --silent --runInBand",
    "lint": "eslint client/",
    "lint:fix": "yarn lint --fix",
    "postinstall": "yarn flow:update",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --content-base build --env=dev",
    "build": "rimraf ./public/* && cross-env NODE_ENV=production webpack --env=prod"
  },
  "lint-staged": {
    "client/**/*.{js,jsx}": [
      "yarn lint:fix --max-warnings=0",
      "yarn test --onlyChanged --passWithNoTests --silent --runInBand"
    ],
    "*.js": "eslint --cache --fix"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tools/jest/jest.setup.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "^3.5.3",
    "empty": "^0.10.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^1.4.8",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-with-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.123.0",
    "flow-coverage-report": "^0.7.0",
    "flow-typed": "2.6.2",
    "html-webpack-plugin": "4.3.0",
    "husky": ">=4",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^3.0.2",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.19.2",
    "babel-polyfill": "6.26.0",
    "bootstrap-sass": "^3.3.7",
    "connected-react-router": "6.8.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
